<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AppTheme" parent="Theme.AppCompat.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/blue_color</item>
        <!-- Customize your theme here. -->
    </style>
    
    <style name="Theme.AppTheme.MyactivityTheme" >
        <item name="colorPrimary">@color/teal_200</item>
    </style>
    <style name="Theme.App.SplashScreen" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/backgroundColor</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_mq_logo_foreground</item>
        <item name="postSplashScreenTheme">@style/Theme.AppTheme</item>
        </style>

</resources>